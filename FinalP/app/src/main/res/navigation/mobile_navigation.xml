<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.finalp.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_notice"
        android:name="com.example.finalp.ui.notice.NoticeFragment"
        android:label="@string/menu_notice"
        tools:layout="@layout/fragment_notice" />

    <fragment
        android:id="@+id/nav_license"
        android:name="com.example.finalp.ui.license.licenseFragment"
        android:label="@string/menu_license"
        tools:layout="@layout/fragment_license" />

    <fragment
        android:id="@+id/nav_settings"
        android:name="com.example.finalp.ui.settings.SettingsFragment"
        android:label="@string/menu_settings"
        tools:layout="@layout/fragment_settings" />

    <fragment
        android:id="@+id/nav_myinfo"
        android:name="com.example.finalp.ui.myinfo.MyinfoFragment"
        android:label="@string/menu_myinfo"
        tools:layout="@layout/fragment_myinfo" />

    <fragment
        android:id="@+id/nav_logout"
        android:name="com.example.finalp.ui.logout.LogoutFragment"
        android:label="@string/menu_logout"
        tools:layout="@layout/fragment_logout" />
</navigation>